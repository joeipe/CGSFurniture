    <script src="~/Content/Template/assets/vendor/slick-carousel/slick/slick.js"></script>
    <!-- JS Unify -->
    @*<script src="~/Scripts/Template/hs.core.js"></script>
    <script src="~/Scripts/Template/components/hs.header.js"></script>
    <script src="~/Scripts/Template/helpers/hs.hamburgers.js"></script>
    <script src="~/Scripts/Template/components/hs.dropdown.js"></script>
    <script src="~/Scripts/Template/components/hs.scrollbar.js"></script>
    <script src="~/Scripts/Template/components/hs.tabs.js"></script>
    <script src="~/Scripts/Template/helpers/hs.rating.js"></script>
    <script src="~/Scripts/Template/components/hs.slider.js"></script>
    <script src="~/Scripts/Template/components/hs.carousel.js"></script>
    <script src="~/Scripts/Template/helpers/hs.focus-state.js"></script>
    <script src="~/Scripts/Template/components/hs.count-qty.js"></script>*@
    <!-- JS Customization -->
    @*<script src="~/Scripts/Template/custom.js"></script>*@

    <!-- CGS Init. -->
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/loading-bar.js"></script>
    <script src="~/Scripts/ng-file-upload-shim.js"></script>
    <script src="~/Scripts/ng-file-upload.js"></script>
    <script src="~/Scripts/angular-slick.js"></script>
    <script src="~/Scripts/angular-local-storage.js"></script>
    <script src="~/app/CGSFurnitureApp.js"></script>
    <script src="~/app/authInterceptorService.js"></script>
    <script src="~/app/dataService.js"></script>
    <script src="~/app/authService.js"></script>
    <script src="~/app/fileService.js"></script>
    <script src="~/app/pagerService.js"></script>
    <script src="~/app/mainCtrl.js"></script>
    <script src="~/app/FooterCtrl.js"></script>
    <script src="~/app/Shared/FilterForm/sffDrctv.js"></script>
    <script src="~/app/Shared/FilterForm/sffIndexCtrl.js"></script>
    <script src="~/app/LoginForm/lfIndexCtrl.js"></script>
    <script src="~/app/RegisterForm/rfIndexCtrl.js"></script>
    <script src="~/app/ProductForm/pfIndexCtrl.js"></script>
    <script src="~/app/Settings/CategoryForm/scfIndexCtrl.js"></script>
    <script src="~/app/Settings/CategoryForm/scfTemplateCtrl.js"></script>
    <script src="~/app/Settings/ProductForm/spfIndexCtrl.js"></script>
    <script src="~/app/Settings/ProductForm/spfTemplateCtrl.js"></script>
    <script src="~/app/HomeForm/hfGridIndexCtrl.js"></script>
    <script src="~/app/HomeForm/hfListIndexCtrl.js"></script>
